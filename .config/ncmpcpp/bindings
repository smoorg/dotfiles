def_key "space"
  pause

# vim keys
def_key "k"
  scroll_up

def_key "shift-k"
  select_item
  scroll_up

def_key "j"
  scroll_down

def_key "shift-j"
  select_item
  scroll_down

def_key "h"
  previous_column

def_key "h"
  master_screen

def_key "h"
  jump_to_parent_directory

def_key "l"
  next_column

def_key "l"
  slave_screen

def_key "l"
  enter_directory

